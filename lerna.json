{
  "packages": [
    "packages/*"
  ],
  "version": "0.1.0",
  "npmClient": "npm",
  "command": {
    "version": {
      "createRelease": true,
      "includeMergedTags": true,
      "exact": true,
      "noPush": true,
      "conventionalCommits": true,
      "message": "chore(release): %s",
      "allowBranch": "master"
    }
  }
}
